admin1 = Administrador("Admin", "AdminApellido", "87654321", "02/02/1980", "AdminDirecci贸n", "AdminLocalidad", "54321", "AdminProvincia", "9876543210", "admin@email.com", "adminclave")

# Ejemplo de uso del enum TipoMedioContacto
tipo_medio = TipoMedioContacto.WHATSAPP

# Ejemplo de uso de la clase MedioContacto
medio_contacto = MedioContacto(1, "01/09/2023", "contacto@email.com", "1234567890", "Direcci贸n de contacto", "Contacto")

# Ejemplo de uso de la clase Compra
compra = Compra(1, 1, 1, 1, "01/09/2023", 100.0)

# Ejemplo de uso de la clase EmailService
